{
	"annotation": {
		"prefix": "method",
		"body": [
			"/* ----------------------------$1---------------------------- */",
			"$2() {",
			"\t$3",
			"},",
			"/* ----------------------------$1 end---------------------------- */"
		],
		"description": "创建函数"
	},
	"import utils": {
		"prefix": "utils",
		"body": "import { toast, request$1 } from '../../utils/utils'",
		"description": "引入utils"
	},
	"get request": {
		"prefix": "rqg",
		"body": [
			"request($1, $2).then(res => {",
			"\tconsole.log(res)",
			"\t$3",
			"}).catch(err => {",
			"\ttoast('请求出错')",
			"\tconsole.log(err)",
			"})"
		],
		"description": "get请求"
	},
	"post request": {
		"prefix": "rqp",
		"body": [
			"request($1, 'POST', $2).then(res => {",
			"\tconsole.log(res)",
			"\t$3",
			"}).catch(err => {",
			"\ttoast('请求出错')",
			"\tconsole.log(err)",
			"})"
		],
		"description": "post请求"
	}
}