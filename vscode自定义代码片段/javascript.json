{
	"Print to console": {
		"prefix": "log",
		"body": [
			"console.log('$1')",
			"$2"
		],
		"description": "Log output to console"
	},
	"anno": {
		"prefix": "anno",
		"body": [
			"/* --------------------------------$1-------------------------------- */",
			"/* --------------------------------$1 end-------------------------------- */"
		],
		"description": "自定义注释"
	},
	"method": {
		"prefix": "method",
		"body": [
			"/* ----------------------------$1---------------------------- */",
			"$2() {",
			"\t$3",
			"},",
			"/* ----------------------------$1 end---------------------------- */"
		],
		"description": "创建函数"
	},
	"import utils": {
		"prefix": "applet-utils",
		"body": "import { toast, request$1 } from '../../utils/utils'",
		"description": "小程序引入utils"
	},
	"get request": {
		"prefix": "rq",
		"body": [
			"request($1, $2, $3).then(res => {",
			"\tconsole.log(res)",
			"\t$3",
			"}).catch(err => {",
			"\ttoast('请求出错')",
			"\tconsole.log(err)",
			"})"
		],
		"description": "小程序请求"
	}
}